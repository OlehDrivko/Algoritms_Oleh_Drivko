import math
a= int(input("podaj a"))
b= int(input("podaj b"))
c= int(input("podaj c"))
if a!=0:
    delta = b*b -4*a*c
    if delta>0:
        x1= (-b-math.sqrt(delta))/(2*a)
        x2= (-b+math.sqrt(delta))/(2*a)
        print(x1,x2)
    else:
        if delta == 0 :
            x1 = -b/(2*a)
            print('x1')
        else:
            print('Brak')
else:
    print("To nie jest r√≥wnanie kwadratowe")
